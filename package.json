{
  "name": "carbonldp-workbench",
  "version": "0.8.0",
  "description": "Workbench to administer an on premise installation of Carbon LDP",
  "contributors": [
    {
      "name": "Miguel Arag√≥n",
      "email": "miguel.aragon@base22.com"
    },
    {
      "name": "Alvaro Contreras",
      "email": "alvaro.contreras@base22.com"
    }
  ],
  "license": "BSD-3-Clause",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/CarbonLDP/carbon-workbench.git"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "npm run build:semantic && npm run copy:assets && npm run build:prod",
    "build:prod": "npm run clean:dist && node_modules/.bin/webpack --config config/webpack.prod.js --progress --profile --bail",
    "build:semantic": "node_modules/.bin/gulp --gulpfile src/semantic/gulpfile.js build",
    "clean:dist": "node_modules/.bin/gulp clean:dist",
    "copy:assets": "node_modules/.bin/gulp copy:assets",
    "postinstall": "node scripts/force-semantic-ui-to-install-correctly.js && npm explore semantic-ui -- npm run-script install",
    "test": "karma start",
    "start": "npm run server:dev",
    "server:dev": "node_modules/.bin/webpack-dev-server --config config/webpack.dev.js --inline --progress --profile --watch "
  },
  "devDependencies": {
    "@angular/compiler-cli": "4.3.6",
    "@ngtools/webpack": "1.5.6",
    "@types/codemirror": "^0.0.43",
    "@types/jquery": "^2.0.41",
    "@types/jstree": "^3.3.34",
    "@types/node": "^8.0.19",
    "angular-router-loader": "^0.6.0",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.2.1",
    "copy-webpack-plugin": "^4.0.1",
    "del": "^3.0.0",
    "file-loader": "^0.11.2",
    "gulp": "^3.9.1",
    "html-elements-webpack-plugin": "^1.0.1",
    "html-loader": "^0.5.0",
    "html-webpack-plugin": "^2.28.0",
    "mz": "^2.4.0",
    "node-sass": "^4.5.0",
    "raw-loader": "^0.5.1",
    "run-sequence": "^2.0.0",
    "sass-loader": "^6.0.1",
    "typescript": "^2.3.4",
    "webpack": "^2.7.0",
    "webpack-bundle-analyzer": "^2.8.3",
    "webpack-dev-server": "^2.5.1",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "@angular/common": "4.3.6",
    "@angular/compiler": "4.3.6",
    "@angular/core": "4.3.6",
    "@angular/forms": "4.3.6",
