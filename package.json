{
  "name": "carbonldp-workbench",
  "version": "1.0.0-alpha.3",
  "description": "Workbench to administer an on premise installation of Carbon LDP",
  "contributors": [
    {
      "name": "Miguel Arag√≥n",
      "email": "miguel.aragon@base22.com"
    },
    {
      "name": "Alvaro Contreras",
      "email": "alvaro.contreras@base22.com"
    }
  ],
  "license": "BSD-3-Clause",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/CarbonLDP/carbon-workbench.git"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "npm run build:semantic && npm run build:prod",
    "build:prod": "npm run clean:dist && node_modules/.bin/webpack --config config/prod/webpack.prod.js --progress --profile --bail",
    "build:semantic": "node_modules/.bin/gulp --gulpfile src/semantic/gulpfile.js build",
    "clean:dist": "node_modules/.bin/gulp clean:dist",
    "compodoc": "./node_modules/.bin/compodoc --tsconfig tsconfig.json --output docs/architecture",
    "compodoc:serve": "./node_modules/.bin/compodoc --tsconfig tsconfig.json --output docs/architecture --port=4002 --serve",
    "compodoc:watch": "./node_modules/.bin/compodoc --tsconfig tsconfig.json --output docs/architecture --port=4002 --serve --watch",
    "postinstall": "node scripts/force-semantic-ui-to-install-correctly.js && npm explore semantic-ui -- npm run-script install",
    "test": "karma start",
    "start": "npm run server:dev",
    "server:dev": "node_modules/.bin/webpack-dev-server --config config/dev/webpack.dev.js --progress --watch "
  },
  "devDependencies": {
    "@angular/compiler-cli": "^5.2.4",
    "@compodoc/compodoc": "^1.1.3",
    "@ngtools/webpack": "^1.10.2",
    "@types/codemirror": "^0.0.56",
    "@types/jasmine": "^2.8.6",
    "@types/jasmine-ajax": "^3.1.37",
    "@types/jquery": "^2.0.41",
    "@types/jstree": "^3.3.36",
    "@types/node": "^9.6.5",
    "copy-webpack-plugin": "^4.4.1",
    "css-loader": "^0.28.9",
    "del": "^3.0.0",
    "file-loader": "^1.1.6",
    "gulp": "^3.9.1",
    "html-elements-webpack-plugin": "^1.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.1.0",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.1",
    "karma-jasmine-ajax": "^0.1.13",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.9",
    "mz": "^2.7.0",
    "node-sass": "^4.7.2",
    "raw-loader": "^0.5.1",
    "run-sequence": "^2.2.1",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.20.1",
    "typescript": "^2.6.2",
    "webpack": "^4.3.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.1",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "@angular/common": "^5.2.4",
    "@angular/compiler": "^5.2.4",
    "@angular/core": "^5.2.4",
    "@angular/forms": "^5.2.4",
    "@angular/http": "^5.2.4",
    "@angular/platform-browser": "^5.2.4",
    "@angular/platform-browser-dynamic": "^5.2.4",
    "@angular/router": "^5.2.4",
    "carbonldp": "^5.0.0-alpha.1",
    "codemirror": "^5.34.0",
    "core-js": "^2.5.5",
    "highlight.js": "^9.12.0",
    "jasmine-core": "^2.99.1",
    "jquery": "^2.2.4",
    "js-cookie": "^2.2.0",
    "jstree": "^3.3.5",
    "rxjs": "^5.5.10",
    "semantic-ui": "2.2.10",
    "sparqler": "^0.4.0",
    "webstomp-client": "^1.2.0",
    "zone.js": "^0.8.20"
  }
}
